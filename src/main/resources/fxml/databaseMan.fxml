<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.java.project.company.manager.controllers.DatabaseController">
    <children>
        <HBox alignment="CENTER" spacing="10.0">
            <children>
                <Button fx:id="backupBtn" mnemonicParsing="false" onAction="#backupAction" text="Backup" />
                <Button fx:id="restoreBtn" mnemonicParsing="false" onAction="#restoreAction" text="Restore" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
